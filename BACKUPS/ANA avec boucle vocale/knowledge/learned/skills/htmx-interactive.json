    {"id": "htxi08", "type": "boost", "name": "hx-boost", "description": "Boost links", "pattern": "<body hx-boost=\"true\">\n  <a href=\"/about\">About</a>  <!-- AJAX -->\n  <form action=\"/submit\">...</form>  <!-- AJAX -->\n</body>", "example": "Boost"},
