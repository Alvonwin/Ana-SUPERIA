{
  "1": {
    "inputs": {
      "ckpt_name": "sd_xl_base_1.0.safetensors"
    },
    "class_type": "CheckpointLoaderSimple",
    "_meta": {
      "title": "Load Checkpoint - SDXL Base"
    }
  },
  "2": {
    "inputs": {
      "text": "PROMPT_PLACEHOLDER",
      "clip": ["1", 1]
    },
    "class_type": "CLIPTextEncode",
    "_meta": {
      "title": "Positive Prompt"
    }
  },
  "3": {
    "inputs": {
      "text": "NEGATIVE_PROMPT_PLACEHOLDER",
      "clip": ["1", 1]
    },
    "class_type": "CLIPTextEncode",
    "_meta": {
      "title": "Negative Prompt"
    }
  },
  "4": {
    "inputs": {
      "seed": "SEED_PLACEHOLDER",
      "steps": 25,
      "cfg": 7.5,
      "sampler_name": "dpmpp_2m",
      "scheduler": "karras",
      "denoise": 1.0,
      "model": ["1", 0],
      "positive": ["2", 0],
      "negative": ["3", 0],
      "latent_image": ["5", 0]
    },
    "class_type": "KSampler",
    "_meta": {
      "title": "KSampler - Generate Image (DPM++ 2M Karras - Optimized)"
    }
  },
  "5": {
    "inputs": {
      "width": 1024,
      "height": 1024,
      "batch_size": 1
    },
    "class_type": "EmptyLatentImage",
    "_meta": {
      "title": "Latent Image 1024x1024 (SDXL optimal, x2 upscale = 2048x2048)"
    }
  },
  "6": {
    "inputs": {
      "samples": ["4", 0],
      "vae": ["1", 2]
    },
    "class_type": "VAEDecode",
    "_meta": {
      "title": "VAE Decode"
    }
  },
  "10": {
    "inputs": {
      "model_name": "RealESRGAN_x2plus.pth"
    },
    "class_type": "UpscaleModelLoader",
    "_meta": {
      "title": "Load Upscale Model - RealESRGAN x2 (Fast & Efficient)"
    }
  },
  "7": {
    "inputs": {
      "upscale_model": ["10", 0],
      "image": ["6", 0]
    },
    "class_type": "ImageUpscaleWithModel",
    "_meta": {
      "title": "Upscale with AI Model (x2 - RealESRGAN)"
    }
  },
  "8": {
    "inputs": {
      "upscale_method": "lanczos",
      "width": 1920,
      "height": 1080,
      "crop": "disabled",
      "image": ["7", 0]
    },
    "class_type": "ImageScale",
    "_meta": {
      "title": "Scale to 1920x1080 (Final output, VRAM optimized)"
    }
  },
  "9": {
    "inputs": {
      "filename_prefix": "ARCHON_",
      "images": ["8", 0]
    },
    "class_type": "SaveImage",
    "_meta": {
      "title": "Save Final Image"
    }
  }
}
